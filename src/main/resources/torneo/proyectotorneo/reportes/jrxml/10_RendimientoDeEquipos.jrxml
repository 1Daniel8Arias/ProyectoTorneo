    <query language="sql"><![CDATA[SELECT
    e.nombre AS equipo,
    t.puntos,
    t.ganados,
    t.empates,
    t.perdidos,
    t.goles_a_favor AS gf,
    t.goles_en_contra AS gc,
    (t.goles_a_favor - t.goles_en_contra) AS dg
FROM TABLA_POSICION t
JOIN equipo e ON e.id_equipo = t.id_equipo
ORDER BY t.puntos DESC
]]></query>
